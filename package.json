{
  "name": "project-backend-generator",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "db-migrate up -e pg",
    "start": "babel-node ./src/bin/www.js",
    "pretest": "cross-env NODE_ENV=test && npm run migrate:reset:test && npm run migrate:up:test",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=text-summary mocha -c -r tests/**/*.test.js --timeout 12000 --exit --require @babel/register",
    "clean": "rm -rf dist",
    "build": "npm run clean && babel ./src --out-dir dist --copy-files",
    "prod": "node ./dist/bin/www.js",
    "migrate:up:test": "db-migrate up -e test -m migrations -v",
    "migrate:reset:test": "db-migrate reset -e test -m migrations -v",
    "migrate:up": "db-migrate up -e pg -m migrations -v"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@sendgrid/mail": "^7.4.2",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.10.1",
    "cors": "^2.8.5",
    "date-fns": "^2.21.1",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "faker": "^5.5.3",
    "generate-password": "^1.6.0",
    "http-errors": "~1.6.3",
    "jade": "^0.29.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "nodemailer": "^6.5.0",
    "pg-promise": "^10.10.1",
    "regenerator-runtime": "^0.13.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.12",
    "@babel/register": "^7.13.16",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1"
  }
}
